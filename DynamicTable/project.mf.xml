<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<manifest>
    <application name="DynamicTable">
        <crown name="DynamicTable">
            <desc>user script</desc>
            <serves>
                <event name="OnNewMessages">
                    <param desc="Json formatted string with the table entries" multiplicity="1" name="jsonstring" type="string"/>
                </event>
                <function name="getMessages">
                    <desc/>
                    <return desc="" multiplicity="1" name="return1" type="string"/>
                </function>
                <function name="setSelection">
                    <desc/>
                    <param desc="Json formated data of the selected row" multiplicity="1" name="data" type="string"/>
                </function>
            </serves>
        </crown>
        <meta key="author">AppSpace Samples</meta>
        <meta key="version">1.0</meta>
        <meta key="priority">low</meta>
        <meta key="copy-protected">false</meta>
        <meta key="read-protected">false</meta>
        <entry default="main.lua" path="scripts"/>
    </application>
</manifest>
